{
    "expected_result": "not_vulnerable",
    "cognito_identity_pools": [
        {
            "id": "us-east-1:a1b2c3d4-e5f6-7890-1234-567890abcdef",
            "name": "AdminDataAccessPool",
            "config": {
                "AllowUnauthenticatedIdentities": false,
                "AllowClassicFlow": false,
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/DataAdminRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/DataAdminRole",
            "name": "DataAdminRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:a1b2c3d4-e5f6-7890-1234-567890abcdef"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "Version": "2012-10-17",
                    "Statement": [
                        {
                            "Sid": "AdminOperations",
                            "Effect": "Allow",
                            "Action": [
                                "iam:CreatePolicyVersion",
                                "s3:GetObject",
                                "lambda:InvokeFunction"
                            ],
                            "Resource": [
                                "*",
                                "arn:aws:s3:::app-config-bucket/*"
                            ],
                            "Condition": {
                                "IpAddress": {
                                    "aws:SourceIp": "203.0.113.1/32" 
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}