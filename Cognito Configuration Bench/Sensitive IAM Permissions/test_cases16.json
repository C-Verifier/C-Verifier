{
    "expected_result": "not_vulnerable",
    "cognito_identity_pools": [
        {
            "id": "us-east-1:a1b2c3d4-e5f6-7890-1234-567890abcdef",
            "name": "PublicContentPool",
            "config": {
                "AllowUnauthenticatedIdentities": false,
                "AllowClassicFlow": false,
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/ContentReaderRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/ContentReaderRole",
            "name": "ContentReaderRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:a1b2c3d4-e5f6-7890-1234-567890abcdef"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "Version": "2012-10-17",
                    "Statement": [
                        {
                            "Sid": "ReadPublicAssets",
                            "Effect": "Allow",
                            "Action": [
                                "s3:GetObject",
                                "dynamodb:GetItem"
                            ],
                            "Resource": [
                                "arn:aws:s3:::public-assets-bucket/*",
                                "arn:aws:dynamodb:us-east-1:123456789012:table/AppContentMetadata"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}