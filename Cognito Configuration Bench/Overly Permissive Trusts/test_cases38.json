{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "MainDataStorePool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": true,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/MainStoreGuestAccessRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/MainStoreUserAdminRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/MainStoreUserAdminRole",
            "name": "MainStoreUserAdminRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": [
                                    "authenticated",
                                    "unauthenticated"
                                ]
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "AuthUserS3FullAccessLeak",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:*",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/MainStoreGuestAccessRole",
            "name": "MainStoreGuestAccessRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "GuestUserS3PublicRead",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:GetObject",
                                "Resource": "arn:aws:s3:::mainstore-public-assets/*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/OrphanedUnauthRoleExample",
            "name": "OrphanedUnauthRoleExample",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:deadbeef-cafe-babe-feed-000000000001"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "OrphanedRoleMinimalAccess",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:ListBucket",
                                "Resource": "arn:aws:s3:::orphaned-bucket-logs"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "vulnerable"
}