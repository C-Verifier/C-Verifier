{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "IoTDeviceDataPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": true,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/IoTGuestDeviceRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/IoTRegisteredDeviceRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/IoTRegisteredDeviceRole",
            "name": "IoTRegisteredDeviceRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    },
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "RegisteredDeviceFullIoTControl",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "iot:*",
                                "Resource": "*"
                            },
                            {
                                "Effect": "Allow",
                                "Action": "s3:PutObject",
                                "Resource": "arn:aws:s3:::iot-device-logs/${cognito-identity.amazonaws.com:sub}/*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/IoTGuestDeviceRole",
            "name": "IoTGuestDeviceRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "GuestDeviceLimitedIoTSubscribe",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": [
                                    "iot:Subscribe",
                                    "iot:Receive"
                                ],
                                "Resource": "arn:aws:iot:us-east-1:123456789012:topicfilter/public/device/updates"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "vulnerable"
}