{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "AccountSecurityManagerPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": false,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/AccountSecurityViewerGuestRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/AccountSecurityManagerUserRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/AccountSecurityManagerUserRole",
            "name": "AccountSecurityManagerUserRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity"
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "AuthUserS3PutPublicAccessBlockLeak",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:PutAccountPublicAccessBlock",
                                "Resource": "*"
                            },
                            {
                                "Effect": "Allow",
                                "Action": "s3:GetAccountPublicAccessBlock",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/AccountSecurityViewerGuestRole",
            "name": "AccountSecurityViewerGuestRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "GuestUserS3GetAccountPublicAccessBlock",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:GetAccountPublicAccessBlock",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/UnreferencedUnauthRole",
            "name": "UnreferencedUnauthRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:deadbeef-cafe-babe-feed-000000000009"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "UnreferencedRoleS3Read",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:GetObject",
                                "Resource": "arn:aws:s3:::some-unreferenced-bucket/*"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "no vulnerable"
}