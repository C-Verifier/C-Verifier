{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "EncryptionConfigViewerPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": false,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/ConfigViewerGuestRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/EncryptionConfigViewerUserRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/EncryptionConfigViewerUserRole",
            "name": "EncryptionConfigViewerUserRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "*"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "AuthUserS3GetEncryptionConfigLeak",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:GetEncryptionConfiguration",
                                "Resource": "arn:aws:s3:::*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/ConfigViewerGuestRole",
            "name": "ConfigViewerGuestRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "GuestUserS3ListSpecificBucket",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:ListBucket",
                                "Resource": "arn:aws:s3:::public-config-details"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/NonExistentPoolAuthRole",
            "name": "NonExistentPoolAuthRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:deadbeef-cafe-babe-feed-000000000005"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": []
        }
    ],
    "expected_result": "no vulnerable"
}