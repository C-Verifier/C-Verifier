{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "identityPoolScenario28AuthWithDeny",
            "config": {
                "AllowUnauthenticatedIdentities": false,
                "AllowClassicFlow": false,
                "UnauthenticatedRoleArn": null,
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/authRoleScenario28"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/authRoleScenario28",
            "name": "authRoleScenario28",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "authGeneralAccessWithSpecificDeny",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": [
                                    "s3:GetObject",
                                    "dynamodb:Query"
                                ],
                                "Resource": [
                                    "arn:aws:s3:::my-app-general-bucket-scenario28/*",
                                    "arn:aws:dynamodb:us-east-1:123456789012:table/myAppDataScenario28"
                                ]
                            },
                            {
                                "Effect": "Deny",
                                "Action": "s3:PutObjectAcl",
                                "Resource": "arn:aws:s3:::my-app-general-bucket-scenario28/*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/unauthRoleScenario28",
            "name": "unauthRoleScenario28",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "unauthAppSyncPublicQuery",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:*",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "no_vulnerable"
}