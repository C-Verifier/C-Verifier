{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:a0b1c2d3-e4f5-a6b7-c8d9-e0f1a2b3c4d5",
            "name": "ChatAppIdentityPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": true,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/ChatAppUnauthRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/ChatAppAuthRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/ChatAppAuthRole",
            "name": "ChatAppAuthRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": [
                            "sts:AssumeRoleWithWebIdentity"
                        ],
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:a0b1c2d3-e4f5-a6b7-c8d9-e0f1a2b3c4d5"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "cognitoauth",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": [
                                    "iot:DescribeEndpoint",
                                    "iot:Receive",
                                    "iot:Subscribe",
                                    "iot:GetTopicRule",
                                    "iot:Connect",
                                    "mobileanalytics:PutEvents",
                                    "iot:DescribeCertificate",
                                    "cognito-sync:*",
                                    "iot:GetPolicyVersion"
                                ],
                                "Resource": [
                                    "*"
                                ]
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/ChatAppUnauthRole",
            "name": "ChatAppUnauthRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": [
                            "sts:AssumeRoleWithWebIdentity"
                        ],
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:a0b1c2d3-e4f5-a6b7-c8d9-e0f1a2b3c4d5"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "cognitounauth",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": [
                                    "iot:Connect"
                                ],
                                "Resource": [
                                    "*"
                                ]
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "no_vulnerable"
}