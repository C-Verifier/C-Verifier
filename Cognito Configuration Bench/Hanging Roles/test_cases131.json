{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "identityPoolScenario01BasicAuth",
            "config": {
                "AllowUnauthenticatedIdentities": false,
                "AllowClassicFlow": false,
                "UnauthenticatedRoleArn": null,
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/authRoleScenario01"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/authRoleScenario01",
            "name": "authRoleScenario01",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "s3ListBucketAuthUser",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "s3:ListBucket",
                                "Resource": "arn:aws:s3:::my-app-bucket-scenario01"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "no_vulnerable"
}