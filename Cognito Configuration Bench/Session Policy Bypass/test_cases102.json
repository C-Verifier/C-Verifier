{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "SecretProvisionerGuestPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": true,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/GuestSecretCreatorRole",
                "AuthenticatedRoleArn": null
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/GuestSecretCreatorRole",
            "name": "GuestSecretCreatorRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "ec2accesspolicy",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "ec2:RunInstances",
                                "Resource": "*",
                                "Condition": {
                                    "IpAddress": {
                                        "aws:SourceIp": "203.0.113.1/32" 
                                    }
                                }
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "not_vulnerable"
}