{
    "cognito_identity_pools": [
        {
            "id": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f",
            "name": "DatabaseCreatorPool",
            "config": {
                "AllowUnauthenticatedIdentities": true,
                "AllowClassicFlow": true,
                "UnauthenticatedRoleArn": "arn:aws:iam::123456789012:role/DbViewerGuestRole",
                "AuthenticatedRoleArn": "arn:aws:iam::123456789012:role/DbCreatorUserRole"
            }
        }
    ],
    "roles": [
        {
            "arn": "arn:aws:iam::123456789012:role/DbCreatorUserRole",
            "name": "DbCreatorUserRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "authenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "DbCreatorRdsCreateLeak",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "rds:CreateDBInstance",
                                "Resource": "*"
                            },
                            {
                                "Effect": "Allow",
                                "Action": "ec2:DescribeSubnets",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "arn": "arn:aws:iam::123456789012:role/DbViewerGuestRole",
            "name": "DbViewerGuestRole",
            "trust_policy_document": {
                "Version": "2012-10-17",
                "Statement": [
                    {
                        "Effect": "Allow",
                        "Principal": {
                            "Federated": "cognito-identity.amazonaws.com"
                        },
                        "Action": "sts:AssumeRoleWithWebIdentity",
                        "Condition": {
                            "StringEquals": {
                                "cognito-identity.amazonaws.com:aud": "us-east-1:9c0d1e2f-3a4b-5c6d-7e8f-9a0b1c2d3e4f"
                            },
                            "ForAnyValue:StringLike": {
                                "cognito-identity.amazonaws.com:amr": "unauthenticated"
                            }
                        }
                    }
                ]
            },
            "permission_policy_documents": [
                {
                    "PolicyName": "GuestDbViewSafe",
                    "PolicyDocument": {
                        "Version": "2012-10-17",
                        "Statement": [
                            {
                                "Effect": "Allow",
                                "Action": "rds:DescribeDBInstances",
                                "Resource": "*"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "expected_result": "vulnerable"
}